{
    "version": "v1",
    "status": "OK",
    "data": 
    [
        {%- for dataset in data -%}
        {
            {% include 'basic_single_details.json.jinja2' %}
            "products":
            [
                {%- for prod_id, prod_metadata in dataset.products.items() -%}
                    {
                        "id": "{{ prod_id }}",
                        "description": "{{ prod_metadata.description }}"
                    }
                {{ ", " if not loop.last else "" }}
                {%- endfor -%}
            ]
            }
            {{ ", " if not loop.last else "" }}
        {%- endfor -%}        
    ]
}